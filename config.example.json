{
  "default_conf": {
    "github_token": "YOUR_GITHUB_TOKEN",
    "repo_owner": "BACKUP_TARGET_REPO_OWNER",
    "backup": {
      "type": "gitea",
      "config": {
        "host": "GITEA_HOST",
        "token": "GITEA_TOKEN",
        "auth_username": "GITEA_USERNAME"
      }
    }
  },
  "targets": [
    {
      "owner": "GITHUB_OWNER",
      "token": "GITHUB_TOKEN",
      "repo_owner": "BACKUP_TARGET_REPO_OWNER",
      "backup": {
        "type": "file",
        "config": {
          "dir": "SAVE_DIR",
          "history": "FILE_HISTORY_JSON_PATH"
        }
      }
    }
  ]
}